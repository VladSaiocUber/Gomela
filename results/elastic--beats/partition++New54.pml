typedef Mutexdef {
	chan Lock = [0] of {bool};
	chan Unlock = [0] of {bool};
	chan RLock = [0] of {bool};
	chan RUnlock = [0] of {bool};
	int Counter = 0;}



init { 
	chan child_New540 = [1] of {int};
	run New54(child_New540);
	run receiver(child_New540)
stop_process:skip
}

proctype New54(chan child) {
	bool closed; 
	int i;
	bool state;
	int num_msgs;
	Mutexdef ms_broker_broker_lock;
	run mutexMonitor(ms_broker_broker_lock);
	

	if
	:: true -> 
		goto stop_process
	:: true;
	fi;
	

	if
	:: true -> 
		goto stop_process
	:: true;
	fi;
	goto stop_process;
	stop_process: skip;
	child!0
}

