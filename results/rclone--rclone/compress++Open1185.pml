#define def_var_options  -2 // opt options line 1185
typedef Mutexdef {
	chan Lock = [0] of {bool};
	chan Unlock = [0] of {bool};
	chan RLock = [0] of {bool};
	chan RUnlock = [0] of {bool};
	int Counter = 0;}



init { 
	chan child_Open11850 = [1] of {int};
	run Open1185(def_var_options,child_Open11850);
	run receiver(child_Open11850)
stop_process:skip
}

proctype Open1185(int var_options;chan child) {
	bool closed; 
	int i;
	bool state;
	int num_msgs;
	Mutexdef chunkedReader_mu;
	

	if
	:: true -> 
		goto stop_process
	:: true;
	fi;
	

	if
	:: true -> 
		goto stop_process
	:: true;
	fi;
	run mutexMonitor(chunkedReader_mu);
	

	if
	:: true -> 
		goto stop_process
	:: true;
	fi;
	goto stop_process;
	stop_process: skip;
	child!0
}

