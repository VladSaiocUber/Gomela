typedef Mutexdef {
	chan Lock = [0] of {bool};
	chan Unlock = [0] of {bool};
	chan RLock = [0] of {bool};
	chan RUnlock = [0] of {bool};
	int Counter = 0;}



init { 
	chan child_Run760 = [1] of {int};
	run Run76(child_Run760);
	run receiver(child_Run760)
stop_process:skip
}

