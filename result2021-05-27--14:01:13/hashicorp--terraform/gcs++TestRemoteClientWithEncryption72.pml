typedef Mutexdef {
	chan Lock = [0] of {bool};
	chan Unlock = [0] of {bool};
	chan RLock = [0] of {bool};
	chan RUnlock = [0] of {bool};
	int Counter = 0;}



init { 
	chan child_TestRemoteClientWithEncryption720 = [1] of {int};
	run TestRemoteClientWithEncryption72(child_TestRemoteClientWithEncryption720);
	run receiver(child_TestRemoteClientWithEncryption720)
stop_process:skip
}

proctype TestRemoteClientWithEncryption72(chan child) {
	bool closed; 
	int i;
	bool state;
	int num_msgs;
	Mutexdef rs_mu;
	run mutexMonitor(rs_mu);
		stop_process: skip;
	stop_process: skip;
	child!0
}

